<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER_LEFT" onMouseClicked="#onClick" prefHeight="75.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <ImageView fx:id="imageView" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
      <HBox.margin>
         <Insets left="10.0" />
      </HBox.margin>
   </ImageView>
   <Region HBox.hgrow="ALWAYS" />
   <Label fx:id="nameLabel" text="Label">
      <HBox.margin>
         <Insets right="10.0" />
      </HBox.margin></Label>
   <Region HBox.hgrow="ALWAYS" />
   <Button mnemonicParsing="false" onAction="#start" text="Lancer" />
</HBox>
