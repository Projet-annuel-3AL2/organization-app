<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox alignment="CENTER" prefHeight="142.0" prefWidth="207.0" spacing="15.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <Label text="Creation de projet" />
    <VBox alignment="CENTER" prefHeight="1.0" prefWidth="207.0" spacing="15.0">
        <Label fx:id="errorLabel" text="Impossible de créer un projet" visible="false" />
        <GridPane alignment="CENTER" hgap="10.0" vgap="5.0">
            <TextField fx:id="nameField" maxWidth="200.0" prefWidth="200.0" GridPane.columnIndex="1" />
            <Label text="Nom" textAlignment="RIGHT" />
            <columnConstraints>
                <ColumnConstraints halignment="RIGHT" />
                <ColumnConstraints percentWidth="40.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints />
            </rowConstraints>
        </GridPane>
    </VBox>
    <Button mnemonicParsing="false" onAction="#onCreateProjectButton" text="Créer" />
</VBox>
