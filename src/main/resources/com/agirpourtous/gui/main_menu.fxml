<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="600.0" minWidth="800.0"
      prefHeight="600.0" prefWidth="800.0" spacing="50.0" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1">

   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>
   <HBox alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="15.0">
         <Label fx:id="usernameLabel" text="Username"/>
         <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
               spacing="15.0">
            <Button fx:id="createUserButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                    mnemonicParsing="false" text="Create user"/>
            <Button fx:id="createProjectButton" mnemonicParsing="false" text="Create Project"/>
         </VBox>
         <Button fx:id="disconnectButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                 mnemonicParsing="false" text="Disconnect"/>
      </VBox>
   </HBox>
   <VBox prefHeight="200.0" prefWidth="100.0">
      <Label fx:id="projectLabel" text="Your projects"/>
      <ScrollPane fx:id="projectsHBox" fitToHeight="true" prefHeight="200.0" prefWidth="780.0" vbarPolicy="NEVER">
         <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"/>
      </ScrollPane>
   </VBox>
</VBox>
